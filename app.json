{
  "expo": {
    "name": "Vortex Land",
    "slug": "corevex-app-vortex-land",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./assets/google-info/GoogleService-Info.plist",
      "bundleIdentifier": "dodyy.vortex.app"
    },
    "android": {
      "googleServicesFile": "./assets/google-info/google-services.json",
      "package": "dodyy.vortex.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "extra": {
      "eas": {
        "projectId": "a06e2adb-177c-4598-98cc-ee332483c3aa"
      }
    }
  }
}
